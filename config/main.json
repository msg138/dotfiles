{
  "cacheDirectory": "<home>/.dotfile-cache",
  "backupDirectory": "<home>/.dotfile-backup",
  "tempDirectory": {
    "location": "<home>/.dotfile-tmp",
    "deleteIfExists": true,
    "deleteOnFinish": false
  },
  "keys": {
    "github": {
      "type": "ed25519",
      "private": ".ssh/github_key"
    }
  },
  "directories": [
    {
      "name": ".ssh",
      "symlink": "<home>/.ssh",
      "deleteIfPresent": false
    },
    {
      "name": ".vim",
      "symlink": "<home>/.vim",
      "deleteIfPresent": false,
      "sub": [
        "/pack/vendor/start"
      ]
    },
    {
      "name": ".tmux",
      "symlink": "<home>/.tmux",
      "deleteIfPresent": false,
      "sub": [
        "/plugins"
      ]
    }
  ],
  "git": {
    "repos": [
      {
        "repo": "https://github.com/preservim/nerdtree.git",
        "repoFolder": "nerdtree",
        "location": ".vim/pack/vendor/start"
      },
      {
        "repo": "https://github.com/tmux-plugins/tpm.git",
        "repoFolder": "tpm",
        "location": ".tmux/plugins"
      }
    ]
  },
  "dotfiles": [
    "vimrc.json",
    "bashrc.json",
    "tmuxrc.json"
  ],
  "symlinks": [
    {
      "source": ".vimrc",
      "destination": "<home>/test"
    }
  ]
}
