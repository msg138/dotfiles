{
  "fileName": ".vimrc",
  "symlink": "<home>/.vimrc",
  "lineCommentCharacter": "\"",
  "configValues": [
    {
      "key": "set",
      "value": "nocompatible"
    },
    {
      "key": "set encoding",
      "value": "utf-8",
      "deliminator": "="
    },
    {
      "key": "set",
      "value": "termguicolors"
    },
    {
      "key": "set foldmethod",
      "value": "indent",
      "deliminator": "="
    },
    {
      "key": "set foldlevelstart",
      "value": "30",
      "deliminator": "="
    },
    {
      "key": "set mouse",
      "value": "a",
      "deliminator": "="
    },
    {
      "key": "set colorcolumn",
      "value": "100",
      "deliminator": "="
    },
    {
      "key": "color",
      "value": "catppuccin_mocha"
    },
    {
      "comment": "Syntax Highlighting",
      "key": "syntax",
      "value": "on"
    },
    {
      "comment": "Tab Options"
    },
    {
      "key": "set",
      "value": "autoindent"
    },
    {
      "key": "set",
      "value": "expandtab"
    },
    {
      "key": "set shiftwidth",
      "deliminator": "=",
      "value": "2"
    },
    {
      "key": "set tabstop",
      "deliminator": "=",
      "value": "2"
    },
    {
      "key": "set softtabstop",
      "deliminator": "=",
      "value": "2"
    },
    {
      "key": "set showtabline",
      "deliminator": "=",
      "value": "2"
    },
    {
      "key": "set",
      "value": "smarttab"
    },
    {
      "key": "set",
      "value": "hlsearch"
    },
    {
      "key": "set",
      "value": "number"
    },
    {
      "comment": "Configuration for lightline plugin",
      "key": "set laststatus",
      "value": "2",
      "deliminator": "="
    },
    {
      "key": "set",
      "value": "noshowmode"
    },
    {
      "comment": "Mapping for FuzzyFinder (FZF)",
      "key": "map ;",
      "value": ":FZF<CR>"
    },
    {
      "comment": "Mapping for Reloading Vim config",
      "key": "map <Space>r",
      "value": ":source ~/.vimrc <CR>"
    },
    {
      "comment": "Mapping for Sexy Nerd Comments",
      "key": "map <space>/",
      "value": ":call nerdcommenter#Comment('x', 'toggle')<CR>"
    },
    {
      "comment": "Mapping for NERDTree Toggle",
      "key": "map <C-t>",
      "value": ":NERDTreeToggle<CR>"
    },
    {
      "comment": "Mapping for move-less to scroll without moving edit cursor",
      "key": "noremap \"",
      "value": ":call MoveLessMode()<CR>"
    },
    {
      "comment": "Required to prevent overwriting of shiftwidth in markdown files",
      "key": "let g:markdown_recommended_style",
      "value": "0",
      "deliminator": "="
    },
    {
      "comment": "UltiSnips rebinding To prevent clashing with YouCompleteMe plugin",
      "key": "let g:UltiSnipsExpandTrigger",
      "value": "\"<C-Space>\"",
      "deliminator": "="
    },
    {
      "key": "let g:UltiSnipsJumpForwardTrigger",
      "value": "\"<C-j>\"",
      "deliminator": "="
    },
    {
      "key": "let g:UltiSnipsJumpBackwardTrigger",
      "value": "\"<C-k>\"",
      "deliminator": "="
    },
    {
      "key": "let g:UltiSnipsEditSplit",
      "value": "\"Vertical\"",
      "deliminator": "="
    },
    {
      "comment": "Bullets.vim binding override",
      "key": "let g:bullets_custom_mappings",
      "value": "[['imap', '<C-l>', '<Plug>(bullets-demote)'],['imap', '<C-h>', '<Plug>(bullets-promote)']]",
      "deliminator": "="
    }
  ]
}
