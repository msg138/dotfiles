{
  "fileName": ".bashrc",
  "symlink": "<home>/.bashrc",
  "lineCommentCharacter": "#",
  "configValues": [
    {
      "key": "case $- in",
      "value": "*i*) ;;",
      "deliminator": "\n"
    },
    {
      "key": "*) return;;",
      "value": "esac",
      "deliminator": "\n"
    },
    {
      "key": "HISTSIZE",
      "value": "1000",
      "deliminator": "="
    },
    {
      "key": "HISTFILESIZE",
      "value": "2000",
      "deliminator": "="
    },
    {
      "key": "PS1",
      "value": "'\\[\\033[01;32m\\]\\u@\\h\\[\\033[00m\\]:\\[\\033[01;34m\\]\\w\\[\\033[00m\\]\\$ '",
      "deliminator": "="
    },
    {
      "key": "alias ll",
      "value": "'ls -lah --color=auto'",
      "deliminator": "="
    },
    {
      "key": "export BUN_INSTALL",
      "value": "\"$HOME/.bun\"",
      "deliminator": "="
    },
    {
      "key": "export PATH",
      "value": "$BUN_INSTALL/bin:$PATH:<this-dir>/bin",
      "deliminator": "=",
      "replaceVariables": true
    }
  ]
}
